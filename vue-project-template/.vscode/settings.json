{
    "eslint.validate": [
        "vue",
        "javascript",
        "css"
    ],
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "html.format.enable": false,
    "javascript.format.enable": false,
    "eslint.format.enable": true,
    "vetur.format.enable": false,
    "editor.tabSize": 2,
}